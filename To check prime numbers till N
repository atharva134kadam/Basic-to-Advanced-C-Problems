//To check prime numbers till N:-
#include<iostream>
using namespace std;
int main(){
    
    int a,i,j;

    cout<<"Enter a number: ";
    cin>>a;

    for(i=2 ; i<=a ; i++){

        bool isPrime  = true;

        for(j=2 ; j<i ; j++){

            if(i%j==0){
                isPrime = false;
                break;
            }

        }
        if(isPrime){
            cout<<i<<" ";
        }
    }

    return 0;
    
}
