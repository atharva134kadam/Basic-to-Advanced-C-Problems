//Find the duplicate from array:-
#include<iostream>
using namespace std;
int main()
{
    int arr[100],size;

    cout<<"Enter the size of array: ";
    cin>>size;

    if(size<=0 || size>100)
    {
        cout<<"Invalid size!!";
        return 1;
    }

    cout<<"Enter the array elements: ";
    for(int i=0 ; i<size ; i++)
    {
        cin>>arr[i];
    }

    cout<<"Duplicates are: ";
    for(int j=0 ; j<size ; j++)
    {
        int duplicate = 0;
        for(int k=j+1 ; k<size ; k++)
        {
            if(arr[j] == arr[k])
            {
                cout<<arr[j];
            }
        }
    }
}
