//To find maximum of three numbers:-
#include<iostream>
using namespace std;
int main(){

    int a,b,c;

    cout<<"Enter A: ";
    cin>>a;
    cout<<"Enter B: ";
    cin>>b;
    cout<<"Enter C: ";
    cin>>c;

    if(a>b && a>c){
        cout<<"Greatest is: "<<a;
    }
    else if(b>a && b>c){
        cout<<"Greatest is: "<<b;
    }
    else{
        cout<<"Greatest is: "<<c;
    }

    return 0;

}
