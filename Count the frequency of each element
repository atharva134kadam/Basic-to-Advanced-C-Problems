//Count the frequency of each element:-
#include<iostream>
using namespace std;
int main()
{
    string str;
    int len;

    cout<<"Enter a string: ";
    getline(cin , str);

    len = str.length();

    for(int i=0 ; i<=(len-1) ; i++)
    {
        int temp = str[i];
        int count=0;
        for(int j=0 ; j<=(len-1) ; j++)
        {
            if(temp == str[j])
            {
                count++;
            }
        }
        cout<<"Frequency of "<<str[i]<<" is "<<count<<endl;
    }
}
